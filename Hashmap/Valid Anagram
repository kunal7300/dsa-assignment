bool isAnagram(string s, string t) {
        vector<int>freq(26,0);

        for(char c  : s)
        {
            freq[c-'a']++;
        } 

          for(char c : t)
        {
            freq[c-'a']--;
        } 

        for(int count : freq)
        {
            if(count!=0)
            {
                return false;
            }
        }
        
        return true;
    }

t.c=o(n)
s.c=o(1)
