 int bin(int n )
    {  
       int count=0;
       while(n)
       {
          n&=(n-1);
          count++;
       }

       return count;

        // return  __builtin_popcount(n);
    }
    vector<int> countBits(int n) {
          
          vector<int>an;

          for(int i=0 ; i<=n ; i++)
          {
            an.push_back(bin(i));
          } 
         return an;

    
    }

t.c=o(nlogn)
  s.c=o(n)
