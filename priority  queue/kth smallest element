int kthSmallest(vector<int> &nums, int k) {
        // code here
        priority_queue<int, vector<int>, greater<int>>p;
        
        for(int i=0; i<nums.size(); i++){
            p.push(nums[i]);
        }
        for(int i=1; i<k; i++){
            p.pop();
        }
        return p.top();
    }
t.c=o(n)
s.c=o(1)
